@prefix pc: <http://provchain.org/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix trace: <http://example.org/trace#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Simple supply chain test data without blank nodes
# This demonstrates a basic milk supply chain trace

# Farmer and milk batch
trace:milkBatch001 a trace:ProductBatch ;
    trace:hasBatchID "MB-2025-001" ;
    trace:producedAt "2025-08-09T06:00:00Z"^^xsd:dateTime ;
    prov:wasAttributedTo trace:FarmerSmith .

trace:FarmerSmith a trace:Farmer ;
    prov:label "Smith Dairy Farm" .

# Processing activity
trace:uhtProcessing001 a trace:ProcessingActivity ;
    prov:used trace:milkBatch001 ;
    prov:wasAssociatedWith trace:UHTFactory ;
    trace:recordedAt "2025-08-09T10:30:00Z"^^xsd:dateTime .

trace:UHTFactory a trace:Manufacturer ;
    prov:label "Premium UHT Processing Co." .

# Final product
trace:uhtMilkProduct001 a trace:ProductBatch ;
    trace:hasBatchID "UHT-2025-001" ;
    prov:wasGeneratedBy trace:uhtProcessing001 ;
    trace:lotDerivedFrom trace:milkBatch001 ;
    trace:producedAt "2025-08-09T11:00:00Z"^^xsd:dateTime .

# Quality check
trace:qualityCheck001 a trace:QualityCheck ;
    prov:used trace:uhtMilkProduct001 ;
    prov:wasAssociatedWith trace:QCInspector ;
    trace:recordedAt "2025-08-09T11:15:00Z"^^xsd:dateTime .

trace:QCInspector a trace:TraceAgent ;
    prov:label "Quality Control Inspector" .
